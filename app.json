{
  "expo": {
    "name": "Codey AI Chat",
    "slug": "codey-ai-chat",
    "version": "1.0.0",
    "description":"Codey is chat bot for coders and all powered by openai",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1c202a"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#1c202a"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
